{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#bb9af7",
          "template": "{{.Icon}}  "
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#bb9af7",
          "template": "{{ .UserName }} / {{ .HostName }} "
        },
        {
          "foreground": "#bb9af7",
          "style": "diamond",
          "template": "/ {{ .Name }} ",
          "type": "shell"
        },
        {
          "foreground": "#0db9d7",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": " \u26A1 {{ .HEAD }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#7aa2f7",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": "<#b9f27c>\ueaf7</>  <b>{{ .Path }}</b> ",
          "type": "path"
        },
        {
          "foreground": "#acb0d0",
          "style": "plain",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
